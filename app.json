{
  "expo": {
    "name": "Movie Title Translator",
    "slug": "movie-translator-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#dbdbd5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.plunketk.movietranslator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#dbdbd5"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.plunketk.movietranslator",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      "@react-native-firebase/app"
    ],
    "extra": {
      "eas": {
        "projectId": "fc473735-3456-4cde-8c8d-c4090d5f2c75"
      }
    }
  }
}
